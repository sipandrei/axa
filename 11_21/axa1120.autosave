            <ITEXT CH="Paradox"/>
            <para PARENT="subtitlu" ALIGN="1"/>
            <ITEXT CH="Anonim"/>
            <para PARENT="autor" ALIGN="2"/>
            <ITEXT CH="Un petic de iarb odihnete pe mormntul gol,"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="ntuneric absorbitor n cripta sufletului meu"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="O pal de vnt mereu cutreietoare"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Un strigt de lupt, un ipte de ajutor"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Am fost i fiic am fost i mam"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Cci mam singur mi-am fost,"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Acum sufletu-mi atrn-n poal"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Un motan bleg, nefolositor."/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Am fost poet, am fost dezamgire"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="C-a mea dezamgire am fost"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="i poeziile mele nu au rim"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Din argint au devenit rugin"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="A vrea s fiu o stea cluzitoare,"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Cci eu direcia n-o cunosc."/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="A vrea s pot fugi spre mare"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Ca s-mi scuip regrete-amare."/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="i-a vrea s vd cum e s pot zbura"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Fr s cad, s tremur nencetat"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Vocile din capul meu cnt balade,"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Fac ritualuri i demoni-i invoac"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Aduc iadul n mintea mea i pe Pmnt,"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Umplndu-mi inima de foc i-orare"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Visele mi le prjesc, le fac scrum"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Dar ca mreul phoenix nemuritor"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Ele-i revin triumftor"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Am fost trist, am fost fericit  un timp"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="A mea fericire n amrciune-am transformat-o,"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="i din amrciune, fericire am creat"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Lacrimi n ochi i-un zmbet pe fa."/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Sunt un puzzle greu de rezolvat,"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Cu piese care nu se potrivesc."/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Un fenomen paradoxal,"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Un regat fr continent."/>
            <trail PARENT="paragraf fara alineat" ALIGN="1"/>
            <PageItemAttributes/>
        </PAGEOBJECT>
