            <ITEXT CH="am nghiit mrgele de onix"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="care mi se nepeneau n gt"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="numai de dragul dulcii mpcri"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="hai s ne ngropm n amintiri"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="i s invitm vntul s ne dezgroape ntr-o zi"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="s ne mprtie clipele"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="ca pe o cenu, spre toate punctele cardinale"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="poate te voi ierta pe tine"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="poate i eu o s m iert"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="n-are rost s tot ntoarcem clepsidra la nesfrit"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="cci timpul nostru s-a sfrit"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="demonii trebuie s moar singuri"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
