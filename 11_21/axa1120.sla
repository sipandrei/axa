            <ITEXT CH="totul e o incertitudine "/>
            <para PARENT="poezie"/>
            <ITEXT CH="nu-mi amintesc nici ochi "/>
            <para PARENT="poezie"/>
            <ITEXT CH="nici membre nici voci "/>
            <para PARENT="poezie"/>
            <ITEXT CH="doar un simț ciudat"/>
            <para PARENT="poezie"/>
            <ITEXT CH="ca și cum un șoarece "/>
            <para PARENT="poezie"/>
            <ITEXT CH="flămând, săracul, săracul "/>
            <para PARENT="poezie"/>
            <ITEXT CH="ar încerca să roadă "/>
            <para PARENT="poezie"/>
            <ITEXT CH="un stejar din stomac "/>
            <para PARENT="poezie"/>
            <ITEXT CH="care se întâmplă "/>
            <para PARENT="poezie"/>
            <ITEXT CH="să-mi aparțină "/>
            <para PARENT="poezie"/>
            <ITEXT CH="cine sfinți te-a învățat"/>
            <para PARENT="poezie"/>
            <ITEXT CH="să-ți faci scorburi în "/>
            <para PARENT="poezie"/>
            <ITEXT CH="carnea altora"/>
            <para PARENT="poezie"/>
            <ITEXT CH="sunt prost prost prost"/>
            <para PARENT="poezie"/>
            <ITEXT CH="te lăudai în fața mea "/>
            <para PARENT="poezie"/>
            <ITEXT CH="să crezi că știi înseamnă prostie"/>
            <para PARENT="poezie"/>
            <ITEXT CH="și "/>
            <para PARENT="poezie"/>
            <ITEXT CH="ai ochi verzi"/>
            <para PARENT="poezie"/>
            <ITEXT CH="au fost sortite aceleiași zile "/>
            <para PARENT="poezie"/>
            <ITEXT CH="mă răvășești "/>
            <para PARENT="poezie"/>
            <ITEXT CH="dar tot nu înțeleg "/>
            <para PARENT="poezie"/>
            <ITEXT CH="cum tu înțelegi că-s verzi "/>
            <para PARENT="poezie"/>
            <ITEXT CH="dacă ești așa prost "/>
            <para PARENT="poezie"/>
            <ITEXT CH="pe cât insiști "/>
            <para PARENT="poezie"/>
            <ITEXT CH="și cum poți să-ți amintești "/>
            <para PARENT="poezie"/>
            <ITEXT CH="după ce i-ai ros cu-atâta poftă"/>
            <para PARENT="poezie"/>
            <ITEXT CH="fantomă care exhală metafore "/>
            <para PARENT="poezie"/>
            <ITEXT CH="ce crezi că roata nu se-ntoarce"/>
            <para PARENT="poezie"/>
            <ITEXT CH="știu și eu să mă dau "/>
            <para PARENT="poezie"/>
            <ITEXT CH="cu creierii  de prostie "/>
            <para PARENT="poezie"/>
            <ITEXT CH="și să rod burțile grăsane "/>
            <para PARENT="poezie"/>
            <ITEXT CH="ale mortăciunilor ăstora de ființe"/>
            <para PARENT="poezie"/>
            <ITEXT CH="ai ochi căprui"/>
            <para PARENT="poezie"/>
            <ITEXT CH="ce realizare dar "/>
            <para PARENT="poezie"/>
            <ITEXT CH="hopa, scuză-mă"/>
            <para PARENT="poezie"/>
            <ITEXT CH="cred că mi-a rămas inima ta printre dinți"/>
            <para PARENT="poezie"/>
