            <ITEXT CH="Reflecting Inwards"/>
            <para PARENT="subsectiune" ALIGN="1"/>
            <ITEXT CH="de Alex Vraja (12C)"/>
            <para PARENT="autor"/>
            <ITEXT FONTSIZE="10" CH="I'm sipping on sunrise in a closed room,"/>
            <para PARENT="poezie"/>
            <ITEXT FONTSIZE="10" CH="Pondering words that I avoid"/>
            <para PARENT="poezie"/>
            <ITEXT FONTSIZE="10" CH="So you don't remember we speak in different worlds,"/>
            <para PARENT="poezie"/>
            <ITEXT FONTSIZE="10" CH="Lightning up all the same. "/>
            <breakline/>
            <para PARENT="poezie"/>
            <ITEXT FONTSIZE="10" CH="I am left with hands empty, waiting on a pair"/>
            <para PARENT="poezie"/>
            <ITEXT FONTSIZE="10" CH="Yet moving to an idea. "/>
            <para PARENT="poezie"/>
            <ITEXT FONTSIZE="10" CH="If only cause it's me, loosening your mind"/>
            <para PARENT="poezie"/>
            <ITEXT FONTSIZE="10" CH="Isolated in ethereal crowds."/>
            <breakline/>
            <para ALIGN="1"/>
            <ITEXT FONTSIZE="10" CH="We're colored lights reflecting inwards, "/>
            <para PARENT="poezie"/>
            <ITEXT FONTSIZE="10" CH="Searching for the primary. "/>
            <para PARENT="poezie"/>
            <ITEXT FONTSIZE="10" CH="Glowing dark on our moldable skin, "/>
            <para PARENT="poezie"/>
            <ITEXT FONTSIZE="10" CH="Wishing we melted."/>
            <para PARENT="poezie"/>
            <PageItemAttributes/>
        </PAGEOBJECT>
