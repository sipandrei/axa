            <ITEXT CH="Elfii lui Moș Crăciun"/>
            <para PARENT="subtitlu" ALIGN="1"/>
            <ITEXT CH="anonim"/>
            <para PARENT="autor"/>
            <ITEXT CH="- Vai de mine ce-ați făcut?!!!"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Sania moșului a dispărut!"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Unde-i Cometă, Orbitor,"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Rudolf!!!!"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Nu știm Zendy ce s-a întâmplat,"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Totul dintr-o dată a explodat."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- O fi fost Belsnikel la furat,"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Renii și sania el a luat."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Nu se poate-așa ceva,"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="E un elf ca noi, nu ai uitat?"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Într-adevăr are-un metru și o acadea,"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Dar e șiret și viclean."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Să curățăm până nu vine moșul aici,"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Altfel nu mai primim cadouri și biscuiți."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="-  Judy, du-te și curăță aerul,"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Lestor șterge mizeria din grajd."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Nick, pe unde ai umblat?"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Sania lu’ Moșu a explodat,"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Am o sarcină să-ți dau."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Du-te-n camera doamnei Crăciun"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Și citește-n tăciuni"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Întreabă cine dezastrul a provocat"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Dar asta nimeni nu trebuie aflat."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Jojo ce-ai făcut aici!!?"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Totu-i plin de lipici!"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Nu am fost eu Zendy, crede-mă,"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Nu am fost eu"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Ba Jojo, tu ai fost!"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Ai mâncat toate acadelele de pe brad!"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Eu v-am spus că e un spart,"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Cum altfel vi l-ați fi putut imagina"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Gata Jojo e destul,"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Ai mâncat berechet pentru dejun"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="(Jojo începe să plângă)"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Nimeni nu-l iubește pe amărâtul Jojo (zbierete)"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Iartă-mă Jojo, nu am vrut,"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Vino încoace să te sărut"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- E moșul, ce ne facem, a venit!!!"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Asta-i nemaipomenit."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Zici că-i ceva ne-nregulă cu el"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Simte când e ceva rău."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Ce faceți, dragii mei elfi"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Unde-s renii și sania?"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Ne pare rău moșule, a dispărut!"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Cum adică a dispărut?"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Vă țineți de șotii sau ați mâncat biscuiți din lut?"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Nu moșule, adevărat."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Vai de mine, ce mă fac … (suspine)"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Mai am de plătit abonament în acadele un veac."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="(Nick coboară de pe scări cu coada neagră)"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Nick, tu ce-ai mai făcut?"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Am picat pe tăciuni în fund…"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="-Vai de mine, zise doamna Crăciun"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Cine le-a dat renilor acadele cu rom?"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Asta i-a făcut invizibili."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- (Toți elfii și moșul în cor) Poftim?"/>
            <para PARENT="Default Paragraph Style"/>
            <PageItemAttributes/>
        </PAGEOBJECT>
