            <ITEXT CH="Contur antitetic"/>
            <para PARENT="subtitlu" ALIGN="1"/>
            <ITEXT CH="Paula Vîrsta"/>
            <para PARENT="autor"/>
            <ITEXT CH="Cuvintele înșirate"/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="mă dureau atât de tare"/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="încât le adoram."/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="Furai bucățele din mine"/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="și le prindeai"/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="cu lipici care nu lipea"/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="pe bolta indiferenței. "/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="M-ai dezgolit de putere "/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="acoperindu-mă cu nimic"/>
