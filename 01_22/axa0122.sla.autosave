            <ITEXT CH="Contur antitetic"/>
            <para PARENT="subtitlu" ALIGN="1"/>
            <ITEXT CH="Paula Vrsta"/>
            <para PARENT="autor"/>
            <ITEXT CH="Cuvintele nirate"/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="m dureau att de tare"/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="nct le adoram."/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="Furai bucele din mine"/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="i le prindeai"/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="cu lipici care nu lipea"/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="pe bolta indiferenei. "/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="M-ai dezgolit de putere "/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="acoperindu-m cu nimic"/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="ca s m mbolnvesc"/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="cu medicamentul iubirii tale."/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="Nevoia aprig de tine"/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="m-a nvaat"/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="cum s m dezndrgostesc. "/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="Sufocat de lacrimi,"/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="mi-am spnzurat"/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="ultimele anse la fericire, "/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="dorindu-mi s triasc."/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="M-am afundat in cunoscut,"/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="unde nimic nu-mi era clar,"/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="spernd ca te voi uita"/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="i mi-am amintit de tine. "/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="Mi-ai colorat viaa, "/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="dei erai crbune."/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="Am aflat ct de urt este frumosul"/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="i am mpletit ziua cu noaptea."/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="M-am vindecat cu iubire otrvitoare."/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="Toate cu ajutorul tu,"/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
            <ITEXT CH="distrugtorul meu."/>
            <para PARENT="Default Paragraph Style" ALIGN="3"/>
