            <ITEXT CH="Elfii lui Mo Crciun"/>
            <para PARENT="subtitlu" ALIGN="1"/>
            <ITEXT CH="anonim"/>
            <para PARENT="autor"/>
            <ITEXT CH="- Vai de mine ce-ai fcut?!!!"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Sania moului a disprut!"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Unde-i Comet, Orbitor,"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Rudolf!!!!"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Nu tim Zendy ce s-a ntmplat,"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Totul dintr-o dat a explodat."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- O fi fost Belsnikel la furat,"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Renii i sania el a luat."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Nu se poate-aa ceva,"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="E un elf ca noi, nu ai uitat?"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- ntr-adevr are-un metru i o acadea,"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Dar e iret i viclean."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- S curm pn nu vine moul aici,"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Altfel nu mai primim cadouri i biscuii."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="-  Judy, du-te i cur aerul,"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Lestor terge mizeria din grajd."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Nick, pe unde ai umblat?"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Sania lu Mou a explodat,"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Am o sarcin s-i dau."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Du-te-n camera doamnei Crciun"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="i citete-n tciuni"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="ntreab cine dezastrul a provocat"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Dar asta nimeni nu trebuie aflat."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Jojo ce-ai fcut aici!!?"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Totu-i plin de lipici!"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Nu am fost eu Zendy, crede-m,"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Nu am fost eu"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Ba Jojo, tu ai fost!"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Ai mncat toate acadelele de pe brad!"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Eu v-am spus c e un spart,"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Cum altfel vi l-ai fi putut imagina"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Gata Jojo e destul,"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Ai mncat berechet pentru dejun"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="(Jojo ncepe s plng)"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Nimeni nu-l iubete pe amrtul Jojo (zbierete)"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Iart-m Jojo, nu am vrut,"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Vino ncoace s te srut"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- E moul, ce ne facem, a venit!!!"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Asta-i nemaipomenit."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Zici c-i ceva ne-nregul cu el"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Simte cnd e ceva ru."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Ce facei, dragii mei elfi"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Unde-s renii i sania?"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Ne pare ru moule, a disprut!"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Cum adic a disprut?"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="V inei de otii sau ai mncat biscuii din lut?"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Nu moule, adevrat."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Vai de mine, ce m fac  (suspine)"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Mai am de pltit abonament n acadele un veac."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="(Nick coboar de pe scri cu coada neagr)"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Nick, tu ce-ai mai fcut?"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- Am picat pe tciuni n fund"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="-Vai de mine, zise doamna Crciun"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Cine le-a dat renilor acadele cu rom?"/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="Asta i-a fcut invizibili."/>
            <para PARENT="Default Paragraph Style"/>
            <ITEXT CH="- (Toi elfii i moul n cor) Poftim?"/>
            <para PARENT="Default Paragraph Style"/>
            <PageItemAttributes/>
        </PAGEOBJECT>
