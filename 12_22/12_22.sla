            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Pentru-al Moldovei popor."/>
            <para PARENT="Raspuns Interviu"/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Ajunsau turcii la Vaslui"/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="De unde-aveau să meargă "/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Către cetatea tronului"/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Suceava să o șteargă. "/>
            <para PARENT="Raspuns Interviu"/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Oștirea le era imensă"/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Puterea lor, nemăsurată"/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Dorința lor de recompensă "/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Mereu era necugetată."/>
            <para PARENT="Raspuns Interviu"/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Erau fățarnici și trufași"/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Și se credeau de neînvins "/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Și nu știau că-s observați "/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="De-un adversar distins."/>
            <para PARENT="Raspuns Interviu"/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Stătea pe-un deal și îi privea"/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Ștefan cel Mare, domnitor "/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Avea un plan în mintea sa"/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Care-l făcea biruitor. "/>
            <para PARENT="Raspuns Interviu"/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Deodată liniștea dispare"/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Se-aude al trâmbițelor glas"/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Iar otomanii-n disperare "/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Își trăiesc ultimul lor ceas."/>
            <para PARENT="Raspuns Interviu"/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="De pretutindeni ies oșteni "/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Cu scut și sabia-n mână"/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Ei sunt vitejii moldoveni "/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Din oastea cea mai sfântă."/>
            <para PARENT="Raspuns Interviu"/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Se-aud și focuri mari de tun"/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Păgânii se împrăștie "/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Românii precum un taifun"/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Îi mână spre pustie."/>
            <para PARENT="Raspuns Interviu"/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Călare luptă vitejește "/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Ștefan cel Mare, voievod "/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="De luptă el nu se ferește "/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Își apară al său norod."/>
            <para PARENT="Raspuns Interviu"/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Păgânii se retrag speriați "/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Ei sunt cuprinși de-o tulburare"/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="De lașitate-naripați"/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Oștirea lor dispare."/>
            <para PARENT="Raspuns Interviu"/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Și astfel Ștefan a învins "/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Dușmanul cel nevrednic "/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="De mult curaj este cuprins"/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Al său suflet războinic. "/>
            <para PARENT="Raspuns Interviu"/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Și totuși pentru-așa victorie "/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Atâți români au fost jertfiți "/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Căzuți în luptă și în glorie "/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Ei sunt acum nefericiți. "/>
            <para PARENT="Raspuns Interviu"/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="De-ar ști acei viteji căzuți"/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Jertfiți pentru-a lor țară"/>
            <para PARENT="Raspuns Interviu"/>
            <ITEXT CH="Că a lor sacrificiu mulți "/>
