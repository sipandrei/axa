            <para PARENT="autor"/>
            <ITEXT CH="Într-o rază de amurg"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Tu mi-ai scris pe foi de nuc"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
