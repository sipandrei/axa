            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Unde e ziua aproape uitată"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="în care pentru ultima dată"/>
