            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Cântam în șoaptă la oglinzii"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="În zorii zilei îngândurării mele"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Să zbor frânt și să mă prinzi,"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Și noaptea, la lună și stele,"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
