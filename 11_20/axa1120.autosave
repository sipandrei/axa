            <ITEXT CH="Paradox"/>
            <para PARENT="subtitlu" ALIGN="1"/>
            <ITEXT CH="Anonim"/>
            <para PARENT="autor" ALIGN="2"/>
            <ITEXT CH="Un petic de iarbă odihnește pe mormântul gol,"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Întuneric absorbitor în cripta sufletului meu"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="O pală de vânt mereu cutreietoare"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Un strigăt de luptă, un țipăte de ajutor"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Am fost și fiică am fost și mamă"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Căci mamă singură mi-am fost,"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Acum sufletu-mi atârnă-n poală"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Un motan bleg, nefolositor."/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Am fost poet, am fost dezamăgire"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="C-a mea dezamăgire am fost"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Și poeziile mele nu au rimă"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Din argint au devenit rugină"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Aș vrea să fiu o stea călăuzitoare,"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Căci eu direcția n-o cunosc."/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Aș vrea să pot fugi spre mare"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Ca să-mi scuip regrete-amare."/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Și-aș vrea să văd cum e să pot zbura"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Fără să cad, să tremur neîncetat"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Vocile din capul meu cântă balade,"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Fac ritualuri și demoni-i invoacă"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Aduc iadul în mintea mea și pe Pământ,"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Umplându-mi inima de foc și-orare"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Visele mi le prăjesc, le fac scrum"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Dar ca mărețul phoenix nemuritor"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Ele-și revin triumfător"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Am fost tristă, am fost fericită – un timp"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="A mea fericire în amărăciune-am transformat-o,"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Și din amărăciune, fericire am creat"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Lacrimi în ochi și-un zâmbet pe față."/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Sunt un puzzle greu de rezolvat,"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Cu piese care nu se potrivesc."/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Un fenomen paradoxal,"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Un regat fără continent."/>
            <trail PARENT="paragraf fara alineat" ALIGN="1"/>
            <PageItemAttributes/>
        </PAGEOBJECT>
