            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Două cuvinte și-o chemare,"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Un punct și o sărutare…"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
            <ITEXT CH="Promisiune ți-aș fi făcut,"/>
            <para PARENT="paragraf fara alineat" ALIGN="1"/>
